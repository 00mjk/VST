target: cfrontend/Clight.vo exportclight/Clightdefs.vo

include Makefile

DIRS=lib common  cfrontend flocq/Core flocq/Prop flocq/Calc flocq/Appli exportclight

FILES=$(LIB) $(COMMON) $(CFRONTEND)  $(FLOCQ) exportclight/Clightdefs.v

clight-depend: $(FILES)
	$(COQDEP) $^ \
        | sed -e 's|$(ARCH)/$(VARIANT)/|$$(ARCH)/$$(VARIANT)/|g' \
              -e 's|$(ARCH)/|$$(ARCH)/|g' \
        > .depend

%.vo: %.v
	@echo "COQC $*.v"
	@$(COQC) $*.v
